{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "hammad",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "prod": "rimraf ./dist && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --color --progress",
    "staging": "rimraf ./dist && cross-env NODE_ENV=staging webpack --config webpack/webpack.prod.js --color --progress",
    "develop": "rimraf ./dist && cross-env NODE_ENV=develop webpack --config webpack/webpack.prod.js --color --progress",
    "jsonServer": "node server/index.js",
    "lint": "npx eslint . --ext .js,.jsx",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "heroku-postbuild": "rimraf ./dist && webpack --mode production --config webpack/webpack.prod.js --color --progress",
    "deploy:staging": "git push heroku develop:main",
    "prepare": "husky install"
  },
